{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2017",
    "strict": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "src",
    "paths": {
      "admin/*": ["admin/ts/*"],
      "app/*": ["app/ts/*"],
      "common/*": ["common/ts/*"],
      "login/*": ["login/ts/*"]
    }
  }
}